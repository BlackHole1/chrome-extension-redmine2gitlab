{
  "manifest_version": 2,
  "name": "Redmine2Gitlab",
  "version": "1.0.0",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Redmine to Gitlab issue",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "homepage_url": "https://github.com/liubiantao"
}
